ilme.id) {\r\n            FilmeAPI.atualizarFilme(filme).then(() => {\r\n                this.carregarFilmes();\r\n                this.setState({filmeEmEdicao: null});\r\n            });\r\n            return;\r\n        }\r\n\r\n        FilmeAPI.inserirFilme(filme).then(() => {\r\n            this.carregarFilmes();\r\n            this.setState({filmeEmEdicao: null})\r\n        });\r\n    }\r\n \r\n    render() {\r\n        return (\r\n        <>\r\n        <EstruturaDaPagina title='Controle de Alunos'>\r\n            <Section titulo=\"Cadastro dos Alunos\">\r\n            <>\r\n                {this.state.alunoEmEdicao &&\r\n                    <span>Aluno em Edição: {this.state.alunoEmEdicao.name}</span>\r\n                }\r\n            </>\r\n            </Section>\r\n\r\n            <Section titulo=\"Listagem dos Alunos\"> \r\n                <SearchBar></SearchBar>\r\n                <Listagem alunos={this.state.alunos} editar={this.editarAluno} excluir={this.excluirAluno}></Listagem>\r\n            </Section>\r\n        </EstruturaDaPagina>\r\n           \r\n       </>\r\n           \r\n        )\r\n    }\r\n\r\n}\r\n\r\nexport default PaginaDeControle;\r\n\r\n//o h2 vai ser passado como children da estruturaDaPagina\r\n//tenho que ir na estruturadapagina e dizer que tem um children","D:\\Dev\\Devinhouse\\semana-5\\cadastroaula\\src\\services\\alunos.js",[],"D:\\Dev\\Devinhouse\\semana-5\\cadastroaula\\src\\services\\constants.js",[],"D:\\Dev\\Devinhouse\\semana-5\\cadastroaula\\src\\services\\mirage-server.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"63","replacedBy":"67"},{"ruleId":"65","replacedBy":"68"},{"ruleId":"69","severity":2,"message":"70","line":44,"column":13,"nodeType":"71","messageId":"72","endLine":44,"endColumn":21},{"ruleId":"69","severity":2,"message":"70","line":51,"column":9,"nodeType":"71","messageId":"72","endLine":51,"endColumn":17},"no-native-reassign",["73"],"no-negated-in-lhs",["74"],["73"]